                    android:fontFamily="@font/nanum_b"
                    android:textSize="18sp"/>

                <RelativeLayout
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_weight="1"
                    android:gravity="center">
                    <ImageView
                        android:id="@+id/img_search_like4"
